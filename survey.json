{
    pages: [
     {
      name: "Match",
      elements: [
       {
        type: "html",
        name: "Roster",
        html: "<html>\n\n<head>\n    <title>ROSTER:</title>\n</head>\n\n<body>\n    <div>\n        <h3>ROSTER:</h3>\n        <h4>BDS ESPORTS : Shaiiko, Renshiro, RaFaLe, Elemzje, BriD</h4>\n        <h4>G2 ESPORTS : Pengu, Kantoraketti, UUNO, Virtue, CTZN</h4>\n        <h4></h4>\n        <h4></h4>\n    </div>\n</body>\n\n</html>"
       },
       {
        type: "html",
        name: "Rules",
        html: "<head>\n    <title>RULES:</title>\n</head>\n\n<body>\n    <div>\n        <h4>>two teams, 5 players each total 10 players</h4>\n        <h4>>you have to choose total 5 players.</h4>\n        <h4>>you can only choose max 3 players from one team</h4>\n        <h4>>you can only choose min 2 players from one team</h4>\n        <h4>>if you want to choose only 2 player you can select none from drop down when you are selecting third players for team 1.</h4>\n        <h4>choose captain from 5 players you have selected</caption></h4>\n    </div>\n</body>\n\n</html>"
       },
       {
        type: "text",
        name: "name",
        title: "What is your full name?",
        isRequired: true
       },
       {
        type: "matrixdropdown",
        name: "select player from Team 1",
        visibleIf: "{name} notempty",
        title: "BDS ESPORTS",
        isRequired: true,
        columns: [
         {
          name: "select"
         }
        ],
        choices: [
         {
          value: "Shaiiko",
          text: "Shaiiko",
          enableIf: "{select player from Team 1.player 1.select} <> Shaiiko and {select player from Team 1.player 2.select} <> Shaiiko and {select player from Team 1.player 3.select} <> Shaiiko"
         },
         {
          value: "Renshiro",
          text: "Renshiro",
          enableIf: "{select player from Team 1.player 1.select} <> Renshiro and {select player from Team 1.player 2.select} <> Renshiro and {select player from Team 1.player 3.select} <> Renshiro"
         },
         {
          value: "RaFaLe",
          text: "RaFaLe",
          enableIf: "{select player from Team 1.player 1.select} <> RaFaLe and {select player from Team 1.player 2.select} <> RaFaLe and {select player from Team 1.player 3.select} <> RaFaLe"
         },
         {
          value: "Elemzje",
          text: "Elemzje",
          enableIf: "{select player from Team 1.player 1.select} <> Elemzje and {select player from Team 1.player 2.select} <> Elemzje and {select player from Team 1.player 3.select} <> Elemzje"
         },
         {
          value: "BriD",
          text: "BriD",
          enableIf: "{select player from Team 1.player 1.select} <> BriD and {select player from Team 1.player 2.select} <> BriD and {select player from Team 1.player 3.select} <> BriD"
         },
         {
          value: "none",
          text: "none",
          visibleIf: "{select player from Team 1.player 1.select} notempty and {select player from Team 1.player 2.select} notempty",
          enableIf: "{select player from Team 1.player 1.select} notempty and {select player from Team 1.player 2.select} notempty"
         }
        ],
        rows: [
         "player 1",
         {
          value: "player 2",
          visibleIf: "{select player from Team 1.player 1.select} notempty"
         },
         {
          value: "player 3",
          visibleIf: "{select player from Team 1.player 2.select} notempty"
         }
        ]
       },
       {
        type: "matrixdropdown",
        name: "select player from Team 2",
        visibleIf: "{select player from Team 1.player 3.select} notempty",
        title: "G2 ESPORTS",
        isRequired: true,
        columns: [
         {
          name: "select"
         }
        ],
        choices: [
         {
          value: "Pengu",
          text: "Pengu",
          enableIf: "{select player from Team 2.player 1.select} <> Pengu and {select player from Team 2.player 2.select} <> Pengu and {select player from Team 2.player 3.select} <> Pengu"
         },
         {
          value: "Kantoraketti",
          text: "Kantoraketti",
          enableIf: "{select player from Team 2.player 1.select} <> Kantoraketti and {select player from Team 2.player 2.select} <> Kantoraketti and {select player from Team 2.player 3.select} <> Kantoraketti"
         },
         {
          value: "UUNO",
          text: "UUNO",
          enableIf: "{select player from Team 2.player 1.select} <> UUNO and {select player from Team 2.player 2.select} <> UUNO and {select player from Team 2.player 3.select} <> UUNO"
         },
         {
          value: "Virtue",
          text: "Virtue",
          enableIf: "{select player from Team 2.player 1.select} <> Virtue and {select player from Team 2.player 2.select} <> Virtue and {select player from Team 2.player 3.select} <> Virtue"
         },
         {
          value: "CTZN",
          text: "CTZN",
          enableIf: "{select player from Team 2.player 1.select} <> CTZN and {select player from Team 2.player 2.select} <> CTZN and {select player from Team 2.player 3.select} <> CTZN"
         }
        ],
        rows: [
         "player 1",
         {
          value: "player 2",
          visibleIf: "{select player from Team 2.player 1.select} notempty"
         },
         {
          value: "player 3",
          visibleIf: "{select player from Team 2.player 2.select} notempty and {select player from Team 1.player 3.select} = none"
         }
        ]
       },
       {
        type: "radiogroup",
        name: "select Captain",
        visibleIf: "{select player from Team 2.player 3.select} notempty or {select player from Team 2.player 2.select} notempty",
        title: "Captain",
        isRequired: true,
        choices: [
         {
          value: "{select player from Team 1.player 1.select}",
          text: "{select player from Team 1.player 1.select}"
         },
         {
          value: "{select player from Team 1.player 2.select}",
          text: "{select player from Team 1.player 2.select}"
         },
         {
          value: "{select player from Team 1.player 3.select}",
          text: "{select player from Team 1.player 3.select}",
          visibleIf: "{select player from Team 1.player 3.select} <> none"
         },
         {
          value: "{select player from Team 2.player 1.select}",
          text: "{select player from Team 2.player 1.select}"
         },
         {
          value: "{select player from Team 2.player 2.select}",
          text: "{select player from Team 2.player 2.select}"
         },
         {
          value: "{select player from Team 2.player 3.select}",
          text: "{select player from Team 2.player 3.select}",
          visibleIf: "{select player from Team 2.player 3.select} notempty"
         }
        ]
       }
      ],
      title: "GAMBLE : GAMBIT",
      description: "Make your own team become a gambit"
     }
    ]
   }